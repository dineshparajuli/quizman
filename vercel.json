{
  "version": 2,
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/app.py"
    },
    {
      "src": "/(.*\\.(js|css|ico|jpg|png|svg|html))$",
      "dest": "static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "static/index.html"
    }
  ]
}
